# buttonsgrid.kv
<VideoGrid>
    cols: 1
    rows: 1
    Button:
        text: 'Video'
<ButtonsGrid>:
    cols: 3
    rows: 5
    Button:
        id: btn00
        text: '0,0'
        disabled: True
    Button:
        id:btnUp
        text: 'Adelante'
        on_press: root.forward(btnUp)
    Button:
        id: btn02
        text: '0,2'
        disabled: True
    Button:
        id:btnLeft
        text: 'Izquierda'
        on_press: root.left(btnLeft)
    Button:
        id: btn11
        text: '1,1'
        disabled: True
    Button:
        id:btnRight
        text: 'Derecha'
        on_press: root.right(btnRight)
    Button:
        id: btn20
        text: '2,0'
        disabled: True
    Button:
        id:btnDown
        text: 'Atr√°s'
        on_press: root.back(btnDown)
    Button:
        id: btn22
        text: '2,2'
        disabled: True
<Status>:
    cols: 1
    rows: 1


<ContainerBox>:
    orientation: 'horizontal'
    VideoGrid:
    ButtonsGrid
